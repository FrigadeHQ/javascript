{
  "name": "@frigade/js",
  "version": "0.0.4",
  "description": "The official Javascript SDK for Frigade.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css ./dist",
    "build": "yarn clean && yarn copy-version-number && tsup && yarn copy-files",
    "local-release": "tsup",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "copy-version-number": "node ./scripts/copy-version-number.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrigadeHQ/javascript.git"
  },
  "keywords": [
    "Onboarding",
    "Tutorial",
    "Intro",
    "Checklist",
    "Welcome",
    "Tooltips",
    "Tour",
    "Walkthrough",
    "Javascript"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/jest": "^23.3.1",
    "babel-jest": "^29.4.1",
    "copyfiles": "^2.4.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-config": "^29.3.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.4.1",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.9",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^4.9.4"
  },
  "author": "Frigade Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FrigadeHQ/frigade-js/issues"
  },
  "homepage": "https://github.com/FrigadeHQ/frigade-js#readme",
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "packageManager": "yarn@3.6.2"
}
